// kth maximum element in an  array 
#include <iostream>
using namespace std;

int main() {
    
int n,k;
cout<<"Enter size of the array: ";
cin>>n;
    
int a[n];
cout<<"Enter the array elments: ";
for(int i=0;i<n;i++)
{
    cin>>a[i];
}

for(int i=0;i<n;i++)
 {
    for(int j=i+1;j<n;j++)
    {
        if(a[j]<a[i])
        {
            int temp = a[i];
            a[i] = a[j];
            a[j] = temp;
        }
    }
}

 cout<<"Enter the value of k:";
 cin>>k;
 
cout<<"The kth  maximum elment is: "<<a[k-n];
}
